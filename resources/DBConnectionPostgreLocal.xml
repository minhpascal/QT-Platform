<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>PostgreSQL Local connection definition</comment>

	<entry key="id">PG_LOCAL</entry>
	<entry key="description">PostgreSQL Local</entry>
	<entry key="driver">jdbc:postgresql:</entry>
	<entry key="database">//localhost/</entry>
	<entry key="schema"></entry>
	<entry key="user">postgres</entry>
	<entry key="password">postgres</entry>
	
</properties>
